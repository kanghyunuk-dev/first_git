package ex04;

public class Ex02 {
	public static void main(String[] args) {
		boolean a = true;
		boolean b = !a;
		
		System.out.println(a);
		System.out.println(b);
		
		int c = 10;
		int d = 20;
		
		System.out.println(c);
		System.out.println(d);
		
		// d = d + c
		d += c;
		System.out.println(d);
	}
}

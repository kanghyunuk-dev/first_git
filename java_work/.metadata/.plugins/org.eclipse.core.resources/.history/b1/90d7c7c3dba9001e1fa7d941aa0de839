package ex03;

public class Ex08 {
	public static void main(String[] args) {
		char aa = 'A';
		System.out.println(aa);
		int bb = (int) aa;
		System.out.println(bb);
		char cc = (char) bb;
		System.out.println(cc);
		// 1byte -128 ~ +127
		byte kk = 127;
		System.out.println(kk+1);
		System.out.println((byte)(kk+1));
	}
}

package ex02;

public class Ex01 {
	public static void doA() {
		int c = 30;
		int d = 40;
		System.out.println(c+d);
	}
	//	ctrl + d 한줄삭제
	//	z 뒤로돌아기
	//	y 앞으로가기
	//	w 현재창 닫기
	//	m 화면 최대화, 최소화
	//	f11 실행	
	public static void main(String[] args) {
		int a = 10;
		int b = 20;
		System.out.println(a + b);
		
		doA();
	}
}

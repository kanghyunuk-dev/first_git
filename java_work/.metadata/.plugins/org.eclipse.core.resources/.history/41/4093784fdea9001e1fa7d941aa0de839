package ex03;
/*
 * 
 */
public class Ex09 {
	public static void main(String[] args) {
		double d = 85.4d;
		int score = (int)d;
		System.out.println("score = " + score);
		System.out.println("d = " + d);
		
		int aa = 10;
		double ee = aa;
		System.out.println("aa = " + aa);
		System.out.println("ee = " + ee);
	}
}
